spring.datasource.url=<%= @envar['PLUTO_DATASOURCE_URL'] %>
spring.datasource.username=<%= @envar['PLUTO_DATASOURCE_USERNAME'] %>
spring.datasource.password=<%= @envar['PLUTO_DATASOURCE_PASSWORD'] %>
spring.jpa.hibernate.ddl-auto=none

pluto.aws.sqs.tx_queue=<%= @envar['PLUTO_AWS_SQS_TX_QUEUE'] %>

pluto.eth.client.url=<%= @envar['PLUTO_ETH_CLIENT_URL'] %>

pluto.wallet.filepath=classpath:pluto.json
pluto.wallet.passphrase=<%= @envar['PLUTO_WALLET_PASSPHRASE'] %>

pluto.contract_addr.pluto=<%= @envar['PLUTO_CONTRACT_ADDR_PLUTO'] %>
